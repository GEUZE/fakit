# Make path UNIX like
override SCIOPTA_HOME := $(subst \,/,$(SCIOPTA_HOME))
export TARGET=tarm
export BASETARGET=arm
ifeq ($(SCIOPTA_HOME),)
SCIOPTA_HOME=/tmp/sciopta/arm/1.9.6.8
endif
INC= -I$(SCIOPTA_HOME)/include
INC+= -I$(SCIOPTA_HOME)/include/sciopta/$(BASETARGET)
export IPS_ROOT = $(SCIOPTA_HOME)/ips
# Set host specific rules

export MACH ?=arm-none-eabi-

export CC=$(DISTCC) $(MACH)gcc -c
export AR=$(MACH)ar 
export AS=$(MACH)as
export LD=$(MACH)ld

include Rules.gnu

_CFLAGS += -mthumb -mthumb-interwork
_CFLAGS += -fomit-frame-pointer -mstructure-size-boundary=8
_CFLAGS += --param max-inline-insns-single=12
########################################

export CFLAGS
export ASFLAGS
export INC
export TRAPINTERFACE=1
include Rules
